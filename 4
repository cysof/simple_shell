#include <stdio.h>
#include <unistd.h>
#include <stdbool.h>

/**
 * main - entry point
 * Return: 0
 */
int main(void)
{
	if (isatty(STDIN_FILENO))
	{
		while (1)
		{
			write (STDOUT_FILENO, "$ ", 2);
			FFLUSH(STDOUT);

			char command[256];
			if (fgets(command, sizeof(command), stdin) == NULL)
			{
				break;
			}
			write (STDOUT_FILENO, "YOU ENTERED: %S\", command);
		}
	}
	return (0);
}
